#include <stdio.h>

int main() {
    int n, k, comparisons = 0, index = -1;
    scanf("%d", &n);

    int a[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    scanf("%d", &k);

    for (int i = 0; i < n; i++) {
        comparisons++;
        if (a[i] == k) {
            index = i;
            break;
        }
    }

    if (index != -1)
        printf("Found at index %d\n", index);
    else
        printf("Not Found\n");

    printf("Comparisons = %d\n", comparisons);
    return 0;
}
